.ui-cell {
  margin-left: 30rpx;
  padding-right: 30rpx;
  border-top: 1rpx solid #E4E7E8;
}
.ui-cell:first-child {
  border-top: 0;
}
.ui-cell__hd {
  width: 184rpx;
  margin-right: 20rpx;
  font-size: 28rpx;
  color: #73768B;
}
.ui-cell__bd {
  position: relative;
}
.ui-cell__ft {
  position: relative;
}
.section {
  margin-top: 20rpx;
  background-color: #ffffff;
  border-bottom: 1rpx solid #E4E7E8;
}
.plate .ui-cell__ft {
  width: 140rpx;
}
.plate .ui-cell__bd input  {
  margin-left: 47rpx;
}
.plate .ui-cell__bd:after {
  content: " ";
  position: absolute;
  top: 50%;
  margin-top: -30rpx;
  height: 60rpx;
  width: 1rpx;
  background-color: #E4E7E8;
}
.ui-arrow_bottom:after {
  position: absolute;
  right: 30rpx;
  top: 50%;
  margin-top: -12rpx;
}
.ui-arrow_right {
  padding-right: 20rpx;
}
.ui-arrow_right:after {
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 4rpx;
}
.query-btn {
  margin-top: 60rpx;
}
.vcode input {
  width: 200rpx;
}

.tag {
    float: left;
    line-height: 40rpx;
    box-sizing: border-box;
    font-size: 24rpx;
    padding: 0 17rpx;
    background-color: #C5CAD0;
    border-radius: 20rpx;
    color: #fff;
    margin: 8rpx 20rpx 8rpx 0;
}
.tag:after {
    content: "x";
    margin-left: 10rpx;
}
.tag_default:after {
    display: none;
}

.example-icon {
  width: 64rpx;
  height: 64rpx;
}

.example {
  position: fixed;
  z-index: 100;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(41,41,41,0.80);
}
.example image {
  width: 547rpx;
  height: 375rpx;
}
.ui-btn_default {
  margin-top: 40rpx;
}